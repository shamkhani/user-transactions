{
  
    "name": "roomvue/credit-report-service",
    "description": "Service to manage user credit and transactions.",
    "type": "project",
    "require": {
      "predis/predis": "^1.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/console": "^7.1",
        "symfony/config": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/yaml": "^7.1"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      },
      "files": ["src/AppKernel.php"]
    },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/"
      },
      "files": ["src/AppKernel.php"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
